<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ztw.hxzq.mapper.dzh.StockInfoMapper">

	<resultMap id="FcSecumainMap" type="com.ztw.hxzq.entity.FcSecumain">
		<result column="SecuCode" 		property="secuCode" 		jdbcType="VARCHAR" />
		<result column="SecuAbbr" 		property="secuAbbr" 		jdbcType="VARCHAR" />
		<result column="SecuCategory" 	property="secuCategory" 	jdbcType="VARCHAR" />
		<result column="SecuMarket" 	property="secuMarket" 		jdbcType="VARCHAR" />
	</resultMap>
	
	<select id="getAllSecuCodeInfo" resultMap="FcSecumainMap">
		SELECT
			C6 AS SecuCode,
			C5 AS SecuMarket,
			C8 AS SecuCategory,
			C9 AS SecuAbbr
		FROM
			dzh_dd.tDD0020
		WHERE
			C5 = #{secuMarket, jdbcType=VARCHAR}
		AND C8 = #{secuCategory, jdbcType=VARCHAR}
		AND C12 = 1
		AND FLAG != 1
	</select>
	
	
</mapper>